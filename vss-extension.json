{
    "manifestVersion": 1,
    "id": "vsts-extension-quickstart",
    "name": "VSTS Extension Quickstart",
    "public": false,
    "version": "0.0.1",
    "description": "VSTS Extension Quickstart",
    "publisher": "your-publisher-id",
    "categories": [
        "Code"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "marketplace/images/logo.png"
    },
    "demands": [
        "contribution/ms.vss-dashboards-web.widget-sdk-version-2",
        "contribution/ms.vss-web.charts-service"
    ],
    "contributions": [
        {
            "id": "SimpleAreaChart",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Sample Area Chart",
                "description": "A simple area chart widget with no configuration and hard-coded data.",
                "catalogIconUrl": "marketplace/images/catalogIcon.png",
                "uri": "static/app.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        }
    ],
    "files": [
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "src",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "marketplace",
            "addressable": true
        }
    ],
    "scopes": []
}